{
  "expo": {
    "name": "TREKKING-BUDDIES-FE",
    "slug": "TREKKING-BUDDIES-FE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.webp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.webp",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo.webp"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(Trekking_Buddies) to use your location."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Kodchasan-Medium.ttf"
          ]
        }
      ]
    ]
  }
}
